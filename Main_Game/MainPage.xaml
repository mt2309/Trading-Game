    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="427" d:DesignWidth="1000" Loaded="main_Loaded">

    <Grid x:Name="LayoutRoot" Background="Black">
        <Grid HorizontalAlignment="Center" Name="mainContent" VerticalAlignment="Center">
        </Grid>
        <Grid Height="75" HorizontalAlignment="Stretch" Name="settingBar" VerticalAlignment="Bottom" Width="1000" Margin="0,0,0,0">
        </Grid>
    </Grid>
</UserControl>
