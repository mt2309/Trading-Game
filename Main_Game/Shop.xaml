<UserControl x:Class="Main_Game.Shop"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400" 
    KeyDown="Shop_KeyDown" 
    IsTabStop="True">    
        <Canvas x:Name="theCanvas" Width="400" Height="400" KeyDown="Shop_KeyDown">
        <Image x:Name="ShopBackground" Height="400" Source="Images/cave-030.png"></Image>
        <Image x:Name="mainChar" Canvas.Left="170" Canvas.Top="270" Source="Images/Gothitelle.png" Height="64" ></Image>
    </Canvas>
   
</UserControl>
